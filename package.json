{
  "name": "decky-totp",
  "version": "1.0.0",
  "description": "Show TOTP codes on your Steam Deck",
  "scripts": {
    "prepare": "pip3 install -r requirements.txt -t py_modules",
    "build": "rollup -c"
  },
  "keywords": [
    "decky",
    "plugin",
    "totp",
    "otp",
    "2fa",
    "auth"
  ],
  "author": "NotNite <hi@notnite.com>",
  "license": "MIT",
  "homepage": "https://github.com/NotNite/decky-totp",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^21.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.3",
    "@types/react": "16.14.0",
    "@types/webpack": "^5.28.0",
    "rollup": "^2.77.1",
    "rollup-plugin-import-assets": "^1.1.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "decky-frontend-lib": "^3.24.4",
    "react-icons": "^4.4.0"
  },
  "pnpm": {
